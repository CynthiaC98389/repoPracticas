package org.generation.ejercicioJava;
import java.util.Scanner;
//FIBONACCI

public class Ejercicio2 {

	public static void main(String[] args) {
		
		Scanner scanner = new Scanner(System.in);
        int n;
        System.out.print("ingresa los numeros Fibobacci ");
        n = scanner.nextInt();
        
        int a = 0, b = 1;
        
        System.out.println("Fibonacci " + n + " terms:");

        int count = 1;
        do {
            System.out.print(a + " ");
            
        
            int next = a + b;
            a = b;
            b = next;
            
            count++;
        } while (count <= n); 
        scanner.close(); 
    }

	}


